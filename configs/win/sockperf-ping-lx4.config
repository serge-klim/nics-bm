destination=192.168.13.31:5001
destination-mac=EC:0D:9A:98:B2:D2
tx-mempool-size=2048
number-of-packets=1000000
wait-for=5s


[mlx5_pci]
info.default_rxportconf.nb_queues=1
info.default_rxportconf.burst_size=1
info.default_txportconf.nb_queues=1
info.default_txportconf.burst_size=1
rx_reconfig_hint=65000
next_src_port=5000
#drivers/common/mlx5/mlx5_devx_cmds.c:999 
clock_hz=156250449 # hca_attr->dev_freq_khz = 156250

[eal]
l=16-26
n=8
a=0000:16:00.1

[Sinks.1]
Destination=Console
Format=[%Channel%] %TimeStamp% %Message% 
Filter=%Channel% != "stats" | %Severity% >= info

[Sinks.2]
Destination=TextFile
FileName=logs.win/lx4/mx-lx4-%Y-%m-%d_%H-%M-%S.%N.log
AutoFlush=true
Format=[%Severity%]  %Message%
Filter=%Channel% != "stats"

[Sinks.3]
Destination=TextFile
FileName=logs.win/lx4/mx-lx4-stats-%Y-%m-%d_%H-%M-%S.%N.log
AutoFlush=true
Format=[%Severity%]  %Message%
Filter=%Channel% = "stats"